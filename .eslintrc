{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "env": {
    "jest": true
  },
  "rules": {
    "no-prototype-builtins": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "no-await-in-loop": 0,
    "arrow-parens": 0,
    "import/no-cycle": 0,
    "no-restricted-syntax": 0,
    "import/prefer-default-export": 0
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "esversion": 6
  }
}
